(let codeComponentStyle (style.create {
            width:"100%",
            backgroundColor: "23|24|29|255",
            padding:"10px",
          
        }
))

(let oneRow (style.create {
            color:"127|190|219|255",
            fontSize:"12px",
            lineHeight:"12px"
        }
))


(function (CodeComponent props)
  (let code props.code)
  
  (let codeRows [])
  (let oneCodeRow "")
  (let wrapFlag "
")

  (for (let i 0) (less  i code.length) (+= i 1)
      (if (== code[i] wrapFlag)
          (begin 
            (codeRows.push oneCodeRow)
            (set oneCodeRow "")
          )
          (begin
            (+= oneCodeRow code[i])
          )
      )
  )
  (if (more oneCodeRow.length 0)
      (codeRows.push oneCodeRow)
      (console.log "else")
  )

  (let showRows [])
  (console.log "----------------" codeRows.length codeRows)
  (for (let k 0) (less k codeRows.length) (+= k 1)
    (let oneRowCode codeRows[k])
    (showRows.push  <div className={oneRow}>{oneRowCode}</div>)
  )
  (console.log "----------------" showRows)
  <div className={codeComponentStyle}>
      {showRows}
  </div>
)


(export {
  CodeComponent
})