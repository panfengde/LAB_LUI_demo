
(import store from "../../store/index.lab")

(let x 10)
(let y 10)
(let canvasDOm 0)

(function  (CanvasTest props propsMethods)
   <div>
    <canvas
          ref={(lambda (x)
            (set canvasDOm x)
          )}
          style={{width:"400px",height:"400px", border:"1px",backgroundColor: "40|80|144|255",}}
          width="400" height="400"></canvas>
          <button
            onClick={(lambda ()
                            (console.log "-----canvas---")
                            (set x (+ x 3))
                            (set y (+ y 7))
                            (for (let i 0) (less i 800) (+= i 10)
                                (for (let k 0) (less k 800) (+= k 10)
                                    (canvasDOm.point [i k])
                                )
                            )

                            (for (begin (let i 0) (let length 1) ) (less i 800) (+= i 40)
                                (+= length 1)
                                (for (let a_k 0) (less a_k 800) (+= a_k 40)
                                    (canvasDOm.arc [i (+ a_k 5)] length)
                                )
                            )
                            
                            (app.update  )
                        )
                    }
          >绘制按钮</button>
   </div>
)

(export {
  CanvasTest:CanvasTest
})